<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
   
     <style name="MyActionBarTabTextStyle" parent="android:style/Widget.Holo.Light.ActionBar.TabText">
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
     <style name="MyTheme" parent="android:style/Theme.Light.NoTitleBar">
    <item name="android:windowNoTitle">true</item>
</style>
 

    <!-- ActionBar styles -->
    <style name="MyActionBar"
           parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/logo_list</item>

        <!-- Support library compatibility -->
        <item name="background">@drawable/logo_list</item>
    </style>
    <style name="Animations" />
    <style name="Animations.PopDownMenu" />
	
	
	
	
	
	
	

	
	<!-- PopUpMenu -->
	<style name="Animations.PopUpMenu" />
	
	


</resources>
